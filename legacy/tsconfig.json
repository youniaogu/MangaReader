{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2019",
      "es2020.bigint",
      "es2020.date",
      "es2020.number",
      "es2020.promise",
      "es2020.string",
      "es2020.symbol.wellknown",
      "es2021.promise",
      "es2021.string",
      "es2021.weakref",
      "es2022.array",
      "es2022.object",
      "es2022.string",
      "dom"
    ],
    "baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "~/*": ["src/*"]
    }
  }
}
